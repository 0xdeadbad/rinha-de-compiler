module Main

external function println(message)

function fib(n) {
  if n < 2 {
    return n;
  } else {
    return fib(n - 1) + fib(n - 2);
  }
}

function main() {
  let f = fun -> println("hello world from lambda");
  let f2 = fun arg -> println("hello world: " ++ arg);

  println("fib " ++ fib(100000));
  println(f());
  println(f2("pao"));
  println((10 * (14 + 3)));
  println("hello, world");
}
